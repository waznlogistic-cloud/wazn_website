# Tap Payments Integration
# Set to "true" to enable Tap Payments integration
VITE_TAP_ENABLED=false

# Tap Payments Credentials
# Test Credentials (for development/testing)
VITE_TAP_SECRET_KEY=sk_test_YOUR_TEST_SECRET_KEY
VITE_TAP_PUBLIC_KEY=pk_test_YOUR_TEST_PUBLIC_KEY

# Production Credentials (replace with your actual credentials)
# VITE_TAP_SECRET_KEY=sk_live_YOUR_LIVE_SECRET_KEY
# VITE_TAP_PUBLIC_KEY=pk_live_YOUR_LIVE_PUBLIC_KEY

