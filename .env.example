VITE_ARAMEX_ACCOUNT_PIN=
VITE_ARAMEX_ACCOUNT_ENTITY=
VITE_ARAMEX_ACCOUNT_COUNTRY_CODE=SA
VITE_ARAMEX_API_URL=https://ws.aramex.net

# Test/Sandbox Credentials
